  <category>
      <pattern>noreply</pattern>
      <template>Ok, feel free to exit the chat. Have a great day!</template>
  </category>
  
  <category>
      <pattern>*</pattern>
      <template>Please type hi to get started</template>
  </category>

</aiml>
